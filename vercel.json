{
    "headers": [
      {
        "source": "/api/keys",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'self'; connect-src 'self' https://api.stripe.com https://maps.googleapis.com https://adensir.vercel.app;"
          }
        ]
      }
    ],
    "version": 2,
    "builds": [
      { "src": "api/**/*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" }
    ]
  }
  